{
  "name": "web",
  "version": "1.0.0",
  "description": "weblabs",
  "main": "index.js",
  "scripts": {
    "lint": "eslint **/*.js --ignore-path .gitignore --no-error-on-unmatched-pattern",
    "lint:fix": "eslint **/*.js --fix --ignore-path .gitignore",
    "format": "prettier --check . --ignore-path ./.prettierignore",
    "format:fix": "prettier --write . --ignore-path ./.prettierignore",
    "stylelint": "stylelint **/*.css --ignore-path .gitignore",
    "stylelint:fix": "stylelint **/*.css --fix --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/voonya/webLabs/issues"
  },
  "homepage": "https://github.com/voonya/webLabs#readme",
  "devDependencies": {
    "@nodeart/eslint-plugin": "^1.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "2.4.0",
    "sonarjs": "^1.0.0",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0"
  }
